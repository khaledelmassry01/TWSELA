# ===================================
# Twsela Docker Environment Variables
# ===================================
# Copy this file to .env and update the values for your environment.
# NEVER commit the .env file to version control.

# --- MySQL ---
MYSQL_ROOT_PASSWORD=change_me_root_password
MYSQL_PASSWORD=change_me_twsela_password

# --- Grafana ---
GRAFANA_ADMIN_PASSWORD=change_me_grafana_password

# --- Spring Boot Application ---
DB_URL=jdbc:mysql://localhost:3306/twsela?useSSL=false&serverTimezone=UTC&allowPublicKeyRetrieval=true
DB_USERNAME=root
DB_PASSWORD=change_me_db_password
JWT_SECRET=change_me_jwt_secret_at_least_32_chars

# --- Redis ---
# REDIS_PASSWORD=change_me_redis_password
